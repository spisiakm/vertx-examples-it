{
  "name": "web-auth",

  "tags": ["web", "auth", "shiro"],

  "libraries": [
    "${dependencies}/slf4j-api",
    "${dependencies}/vertx-auth-shiro",
    "${dependencies}/shiro-core"
  ],



  "executions": {

    "java" : {
      "directory" : "${base}/target/classes",
      "command" : "java -jar ${jarFile} run io.vertx.example.web.auth.Server"
    }

  },

  "grace-text": "Succeeded in deploying verticle",
  "client-check": "web-auth-check.groovy"
}